        </activity>
        <activity android:name="com.example.wankerbank.Transfer" >
        </activity>
        <activity android:name="com.example.wankerbank.Register" >
        </activity>
        <activity android:name="com.example.wankerbank.Login" >
        </activity>
        <activity android:name="com.example.wankerbank.Dashboard" >
        </activity>
        <activity android:name="com.example.wankerbank.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>